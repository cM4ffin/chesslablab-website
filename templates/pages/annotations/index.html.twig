{% extends 'layouts/default.html.twig' %}

{% block main %}
  <div class="col-md-6">
    <div id="chessboard"></div>
  </div>
  <div class="col-md-6">
    <ul class="nav nav-tabs" id="annotationsTabs" role="tablist">
      <li class="nav-item" role="presentation">
        <button class="nav-link active" id="main-annotations-tab" data-bs-toggle="tab" data-bs-target="#mainAnnotationsTab" type="button" role="tab" aria-controls="mainAnnotationsTab" aria-selected="true">
          Main
        </button>
      </li>
    </ul>
    <div class="tab-content" id="annotationsTabsContent">
      <div class="tab-pane show active" id="mainAnnotationsTab" role="tabpanel" aria-labelledby="main-annotations-tab">
        {% include 'partial/panel/rav_panel.html.twig' %}
      </div>
    </div>
  </div>
  {% block javascripts %}
    {% block importmap %}
      {{ importmap('js/pages/annotations/index.js') }}
    {% endblock %}
  {% endblock %}
{% endblock %}
